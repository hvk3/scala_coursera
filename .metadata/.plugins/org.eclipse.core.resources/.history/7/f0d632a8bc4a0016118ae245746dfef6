package week3

object rationals {
	
	gcd(2, 5) //> res0: Int = 1
	val x = new Rational(1, 2) //> x  : week3.Rational = week3.Rational@b684286
	x.numer //> res1: Int = 1
}

class Rational(x: Int, y: Int) {
	def numer = x
	def denom = y
	
	def gcd(a: Int, b: Int): Int =
		if (b == 0) a
		else gcd(b, a % b) //> gcd: (a: Int, b: Int)Int

	def add(that: Rational) =
		new Rational(numer * that.denom + denom * that.numer, denom * that.denom)
	def toString = 
		val g = 
}