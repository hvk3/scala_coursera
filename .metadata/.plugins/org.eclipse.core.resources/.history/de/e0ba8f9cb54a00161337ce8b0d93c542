package week2

object exercise3 {
	def prod(f: Int => Int)(a: Int, b: Int): Int = {
		if (a > b) 1
		else f(a) * prod(f)(a + 1, b)
	} //> prod: (f: Int => Int)(a: Int, b: Int)Int
	prod(x => x * x)(3, 4) //> res0: Int = 144
	//> factorial: (n: Int)Int
	def factorial(n: Int) = prod(x => x)(1, n)
	factorial(5) //> res1: Int = 120

	def mapReduce(f: Int => Int)
}